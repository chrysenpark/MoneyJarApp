\n        });\n};\n\nconst getJarName = (jarInfo) => {\n    const {name} = jarInfo;\n    return `${name}`\n};\n\nconst getJarAmount = (jarInfo) => {\n    const {amount} = jarInfo;\n    return `${amount}`\n};\n\nfunction Dashboard() {\n    const [jarInfos, setJarInfos] = useState([]);\n\n    useEffect(() => {\n        getUsersJars().then((jarData) => {\n            setJarInfos(jarData);\n            console.log(jarData);\n        }).catch((err) => {\n            console.log(err);\n        });\n    }, []);\n\n\n    return (\n        <Wrapper>\n            <Message message={\"\"} />\n            {jarInfos.map((jarInfo, id) => (\n                <div key ={id}>\n                <Jar\n                    Name = {getJarName(jarInfo)}\n                    Amount = {getJarAmount(jarInfo)}\n                />\n                </div>\n            ))}\n\n\n            <Message message={\"\"} />\n            <Button type =\"button\"  buttonStyle=\"btn--danger--solid\" buttonSize=\"btn-small\">Create Jar</Button>\n            <Button type =\"button\"  buttonStyle=\"btn--solid--solid\" buttonSize=\"btn-small\" onClick={logOut}>log Out</Button>\n        </Wrapper>\n    )\n}\nexport default Dashboard;\n","import React, { Component } from \"react\";\nimport { Router, Switch, Route } from \"react-router-dom\";\n\nimport Login from \"./login\";\nimport Signup from \"./signup\";\nimport DashBoard from \"./Dashboard\";\nimport App from \"./App\";\nimport history from './history';\n\nexport default class Routes extends Component {\n    render() {\n        return (\n            <Router history={history} forceRefresh = {true}>\n                <Switch>\n                    <Route exact path=\"/\" component={App} />\n                    <Route path=\"/login\" component={Login} />\n                    <Route path=\"/signup\" component={Signup} />\n                    <Route path=\"/dashboard/\" component={DashBoard} />\n                </Switch>\n            </Router>\n        )\n    }\n}","